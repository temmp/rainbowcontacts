<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">

		<LinearLayout android:layout_width="fill_parent"
		android:id="@+id/ltest_wifi" android:gravity="center_vertical"
		android:background="@drawable/uplay_new" android:layout_height="fill_parent"
		android:layout_weight="@string/up_new" android:orientation="vertical">
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="horizontal" android:layout_height="wrap_content"
			android:layout_centerVertical="true">
			<RelativeLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:orientation="horizontal" 
				android:layout_weight="7.5">
				<RelativeLayout android:layout_width="2.5dip" android:layout_height="fill_parent" android:id="@+id/uprelative_wifi" 
				android:layout_toLeftOf="@+id/up_text_wifi"></RelativeLayout>	
				<TextView 
				android:id="@+id/up_text_wifi" android:layout_width="wrap_content"
					android:textSize="@dimen/TOP_Font_Size" android:textColor="@color/title"
					android:layout_height="wrap_content" android:text="@string/app_name"
					android:layout_centerInParent="true">
				</TextView>
				
							<ImageView android:src="@drawable/up_icon" android:layout_centerVertical="true"
				android:layout_toLeftOf="@+id/uprelative_wifi" android:layout_width="wrap_content"
				    android:layout_height="wrap_content"/>
			</RelativeLayout>
			<RelativeLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				android:layout_weight="@string/importcontacttolocal_new_thirdimagebutton_relative_weight">
			</RelativeLayout>
		</LinearLayout>
	</LinearLayout>



	<LinearLayout android:layout_width="fill_parent" android:id="@+id/ltest1_wifi"
		android:layout_weight="@string/mid_new" android:orientation="vertical"
		android:background="@drawable/midlay_new"
		android:layout_height="fill_parent">


<LinearLayout android:orientation="horizontal"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="@string/contactmove_toptext_weight">
<LinearLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:layout_weight="@string/contactmove_toptext_left_weight"></LinearLayout>

			<RelativeLayout android:layout_width="fill_parent"
				android:gravity="center_horizontal" android:layout_weight="@string/contactmove_toptext_right_weight"
				android:id="@+id/linearLayout1_wifi" android:layout_height="fill_parent">

				<LinearLayout android:layout_width="fill_parent"
					android:orientation="horizontal" android:id="@+id/contactnumlayout_wifi"
					android:layout_height="wrap_content"
					android:layout_alignParentLeft="true"
					android:layout_centerVertical="true">
					<TextView android:id="@+id/contactCountText_first_wifi" android:layout_width="wrap_content"
						android:textColor="@color/little" android:layout_height="wrap_content"
						android:textSize="@dimen/SUMMARY_Font_Size" android:text="@string/hascontacts">
					</TextView>
					<TextView android:id="@+id/contactCountText_wifi"
						android:layout_width="wrap_content" android:textColor="@color/little"
						android:layout_height="wrap_content" android:textSize="@dimen/SUMMARY_Font_Size"
						android:text="">
					</TextView>
				</LinearLayout>

				<LinearLayout android:layout_width="wrap_content"
					android:orientation="horizontal" android:layout_below="@id/contactnumlayout_wifi"
					android:id="@+id/timelayout_wifi" android:layout_height="wrap_content">
					<TextView android:id="@+id/lasttime_first_wifi" android:layout_width="wrap_content"
						android:textColor="@color/little" android:layout_height="wrap_content"
						android:textSize="@dimen/SUMMARY_Font_Size" android:text="@string/lasttimeop">
					</TextView>
					<TextView android:id="@+id/lasttime_wifi" android:layout_width="wrap_content"
						android:textColor="@color/little" android:layout_height="wrap_content"
						android:textSize="@dimen/SUMMARY_Font_Size" android:text="">
					</TextView>
				</LinearLayout>
			</RelativeLayout>

		</LinearLayout>
<LinearLayout android:layout_height="fill_parent" android:layout_weight="@string/contactmove_button_weight" android:gravity="center" android:layout_width="fill_parent" android:orientation="vertical" android:id="@+id/linearLayout1_wifi">
<ImageButton android:id="@+id/talkbutton1_wifi"
				android:onClick="onSendButton" android:background="@color/touming"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:src="@drawable/onekeysendcontact"
				android:layout_centerInParent="true">
			</ImageButton></LinearLayout>


		<LinearLayout android:layout_width="fill_parent" android:orientation="vertical"
		android:gravity="center_horizontal"
			android:layout_weight="@string/contactmove_button_weight"
			android:layout_height="fill_parent">
		<ImageButton android:id="@+id/talkbutton2_wifi" android:layout_centerInParent="true"
				android:background="@color/touming" android:layout_width="wrap_content"
				android:onClick="onReceiveButton" android:layout_height="wrap_content"
				android:src="@drawable/onekeyreceivecontact"
				>
			</ImageButton>
		</LinearLayout>
								<!-- WiFi版本 -->
		
					<LinearLayout android:orientation="horizontal"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="@string/contactmove_toptext_weight">
<LinearLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:layout_weight="@string/contactmove_toptext_left_weight"></LinearLayout>

			<RelativeLayout android:layout_width="fill_parent"
				android:gravity="center_horizontal" android:layout_weight="@string/contactmove_toptext_right_weight"
				android:id="@+id/linearLayout1_wifi_new" android:layout_height="fill_parent">

				<LinearLayout android:layout_width="fill_parent"
					android:orientation="horizontal" android:id="@+id/contactnumlayout_wifi_new"
					android:layout_height="wrap_content"
					android:layout_alignParentLeft="true"
					android:layout_centerVertical="true">
					<TextView android:layout_width="wrap_content"
						android:textColor="@color/little" android:layout_height="wrap_content"
						android:textSize="@dimen/SUMMARY_Font_Size" android:text="@string/wifi_address">
					</TextView>
					<TextView android:id="@+id/network_ssid_wifi"
						android:layout_width="wrap_content" android:textColor="@color/little"
						android:layout_height="wrap_content" android:textSize="@dimen/SUMMARY_Font_Size"
						android:text="">
					</TextView>
				</LinearLayout>

				<LinearLayout android:layout_width="wrap_content"
					android:orientation="horizontal" android:layout_below="@id/contactnumlayout_wifi_new"
					android:id="@+id/timelayout_wifi_new" android:layout_height="wrap_content">
					<TextView android:layout_width="wrap_content"
						android:textColor="@color/little" android:layout_height="wrap_content"
						android:textSize="@dimen/SUMMARY_Font_Size" android:text="@string/ip_address">
					</TextView>
					<TextView android:id="@+id/local_ip_address_wifi" android:layout_width="wrap_content"
						android:textColor="@color/little" android:layout_height="wrap_content"
						android:textSize="@dimen/SUMMARY_Font_Size" android:text="">
					</TextView>
				</LinearLayout>
			</RelativeLayout>

		</LinearLayout>
		
		
		
		
								<!-- WiFi版本 -->
	</LinearLayout>
		<LinearLayout android:layout_width="fill_parent" android:id="@+id/ltest2_wifi"
		 android:orientation="vertical"
		android:background="@drawable/downlay_new" android:layout_height="fill_parent"
		android:layout_weight="@string/down_new">
			<RelativeLayout android:layout_width="fill_parent"	
		 android:layout_height="fill_parent" android:layout_weight="1.3">
		<TextView android:id="@+id/down_text_wifi" android:layout_centerInParent="true" android:layout_width="wrap_content" android:text="@string/down_text"
		android:layout_height="wrap_content" android:textSize="11dip" android:textColor="@color/down"/>
		
			
		<TextView android:id="@+id/down_text1_wifi" android:layout_centerHorizontal="true" android:layout_below="@+id/down_text_wifi" android:layout_width="wrap_content" android:text="@string/down_text1"
		android:layout_height="wrap_content" android:textSize="11dip" android:textColor="@color/down"/>
		</RelativeLayout>
		<RelativeLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:layout_weight="2.7"></RelativeLayout>
		</LinearLayout>
</LinearLayout>