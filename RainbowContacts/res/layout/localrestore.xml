<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">

		<LinearLayout android:layout_width="fill_parent"
		android:id="@+id/ltest" android:gravity="center_vertical"
		android:background="@drawable/uplay_new" android:layout_height="fill_parent"
		android:layout_weight="@string/up_new" android:orientation="vertical">
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="horizontal" android:layout_height="wrap_content"
			android:layout_centerVertical="true">
			<RelativeLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:orientation="horizontal" 
				android:layout_weight="7.5">
				<RelativeLayout android:layout_width="2.5dip" android:layout_height="fill_parent" android:id="@+id/uprelative" 
				android:layout_toLeftOf="@+id/up_text"></RelativeLayout>	
				<TextView 
				android:id="@+id/up_text" android:layout_width="wrap_content"
					android:textSize="@dimen/TOP_Font_Size" android:textColor="@color/title"
					android:layout_height="wrap_content" android:text="@string/app_name"
					android:layout_centerInParent="true">
				</TextView>
				
							<ImageView android:src="@drawable/up_icon" android:layout_centerVertical="true"
				android:layout_toLeftOf="@+id/uprelative" android:layout_width="wrap_content"
				    android:layout_height="wrap_content"/>
			</RelativeLayout>
			<RelativeLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				android:layout_weight="@string/importcontacttolocal_new_thirdimagebutton_relative_weight">
			</RelativeLayout>
		</LinearLayout>
	</LinearLayout>



	<LinearLayout android:layout_width="fill_parent" android:id="@+id/ltest1"
		android:layout_weight="@string/mid_new" android:orientation="vertical"
		android:background="@drawable/midlay_new"
		android:layout_height="fill_parent">
		<LinearLayout android:id="@+id/linearLayout02"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="@string/importcontacttolocal_new_mid_weight"
			android:orientation="vertical">
			<LinearLayout android:id="@+id/linearLayout05"
				android:layout_width="fill_parent" android:layout_height="fill_parent"
				android:orientation="vertical"
				android:layout_weight="@string/importcontacttolocal_new_firsttextview_linear_weight">
				<RelativeLayout android:id="@+id/RelativeLayout06"
					android:layout_width="fill_parent" android:layout_height="fill_parent"
					android:layout_weight="@string/importcontacttolocal_new_firsttextview_relative_weight">
				</RelativeLayout>
				<RelativeLayout android:id="@+id/RelativeLayout02" 
					android:layout_width="fill_parent" android:layout_height="fill_parent"
					android:layout_weight="@string/importcontacttolocal_new_firsttextview_relative_weight">
<LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
				android:orientation="vertical"><TextView android:id="@+id/importlocal_text1" android:layout_gravity="center_horizontal"
						android:layout_centerInParent="true" android:layout_width="wrap_content"
						android:textColor="@color/little" android:layout_height="wrap_content"
						android:textSize="@dimen/BEGIN_SEARCH_Font_Size" android:text="@string/importcontacttolocal_new_hint3">"
				</TextView>
				<TextView android:id="@+id/waiting_text4" android:layout_gravity="center_horizontal"
						android:layout_centerInParent="true" android:layout_width="wrap_content"
						android:textColor="@color/little" android:layout_height="wrap_content"
						android:textSize="@dimen/BEGIN_SEARCH_Font_Size">"
				</TextView>
				<TextView android:id="@+id/importlocal_text2" android:layout_gravity="center_horizontal"
						android:layout_centerInParent="true" android:layout_width="wrap_content"
						android:textColor="@color/little" android:layout_height="wrap_content"
						android:textSize="@dimen/BEGIN_SEARCH_Font_Size" >"
				</TextView></LinearLayout>
					

				</RelativeLayout>
			</LinearLayout>
			<RelativeLayout android:id="@+id/RelativeLayout03"
				android:layout_width="fill_parent" android:layout_height="fill_parent"
				android:layout_weight="9">
				<ImageButton android:id="@+id/local_ok"
					android:layout_centerInParent="true" android:onClick="onOK"
					android:background="@color/touming" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/ok_3">
				</ImageButton>

			</RelativeLayout>
		</LinearLayout>
		<LinearLayout android:id="@+id/linearLayout03"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:orientation="vertical"
			android:layout_weight="@string/importcontacttolocal_new_firsttextview_linear_weight">
			<RelativeLayout android:id="@+id/RelativeLayout04"
				android:layout_width="fill_parent" android:layout_height="fill_parent"
				android:layout_weight="@string/importcontacttolocal_new_thirdimagebutton_relative_weight">
				<TextView android:id="@+id/importlocal_text3"
					android:layout_centerInParent="true"
					 android:layout_width="wrap_content"
					android:textColor="@color/little" android:layout_height="wrap_content"
					android:textSize="@dimen/SUMMARY_Font_Size" android:text="@string/importcontacttolocal_new_hint1">
				</TextView>

			

			</RelativeLayout>
			<RelativeLayout android:id="@+id/RelativeLayout05"
				android:layout_width="fill_parent" android:layout_height="fill_parent"
				android:layout_weight="@string/importcontacttolocal_new_thirdimagebutton_relative_weight">
				<LinearLayout android:layout_width="fill_parent"
					android:orientation="horizontal" android:id="@+id/linearLayout04"
					android:layout_height="wrap_content" android:layout_centerVertical="true">
					<RelativeLayout android:id="@+id/RelativeLayout07"
						android:layout_width="fill_parent" android:layout_height="fill_parent"
						android:layout_weight="1">
						<ImageButton android:id="@+id/setting_button"
							android:layout_centerInParent="true" android:background="@color/touming"
							android:layout_width="wrap_content" android:onClick="onSetting"
							android:layout_height="wrap_content" android:src="@drawable/highsetting">
						</ImageButton>
					</RelativeLayout>

					<RelativeLayout android:id="@+id/RelativeLayout08"
						android:layout_width="fill_parent" android:layout_height="fill_parent"
						android:layout_weight="1">
						<ImageButton android:id="@+id/local_back"
							android:layout_centerInParent="true" android:onClick="onBack"
							android:background="@color/touming" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:src="@drawable/import_local_back">
						</ImageButton>
					</RelativeLayout>
				</LinearLayout>
			</RelativeLayout>
		</LinearLayout>

	</LinearLayout>
		<LinearLayout android:layout_width="fill_parent" android:id="@+id/ltest2"
		 android:orientation="vertical"
		android:background="@drawable/downlay_new" android:layout_height="fill_parent"
		android:layout_weight="@string/down_new">
			<RelativeLayout android:layout_width="fill_parent"	
		 android:layout_height="fill_parent" android:layout_weight="1.3">
		<TextView android:id="@+id/down_text" android:layout_centerInParent="true" android:layout_width="wrap_content" android:text="@string/down_text"
		android:layout_height="wrap_content" android:textSize="11dip" android:textColor="@color/down"/>
		
			
		<TextView android:id="@+id/down_text1" android:layout_centerHorizontal="true" android:layout_below="@+id/down_text" android:layout_width="wrap_content" android:text="@string/down_text1"
		android:layout_height="wrap_content" android:textSize="11dip" android:textColor="@color/down"/>
		</RelativeLayout>
		<RelativeLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:layout_weight="2.7"></RelativeLayout>
		</LinearLayout>
</LinearLayout>
